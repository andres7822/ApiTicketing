<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Entity\SeguimientoTicket" table="SeguimientoTicket">
        <id name="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="Ticket" type="integer" column="`Ticket`"/>
        <field name="FechaYHora" type="datetime" column="`FechaYHora`"/>
        <field name="Usuario" type="integer" column="`Usuario`"/>
        <field name="Descripcion" type="text" column="`Descripcion`"/>
        <field name="Status" type="integer" column="`Status`"/>
    </entity>
</doctrine-mapping>