<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Entity\CatalogoCondicionesComerciales" table="CatalogoCondicionesComerciales">
        <id name="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="Nombre" length="32" column="`Nombre`"/>
        <field name="DescripcionCondicion" type="text" column="`DescripcionCondicion`"/>
        <field name="Requerida" type="integer" column="`Requerida`"/>
    </entity>
</doctrine-mapping>