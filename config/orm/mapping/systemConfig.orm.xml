<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Entity\systemConfig" table="systemConfig">
        <id name="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="name" length="64" nullable="true" column="name"/>
        <field name="value" length="32" nullable="true" column="value"/>
        <field name="tipeofHTML" length="16" nullable="true" column="tipeofHTML"/>
        <field name="category" type="integer" nullable="true" column="category"/>
        <field name="configKey" length="32" column="configKey"/>
    </entity>
</doctrine-mapping>